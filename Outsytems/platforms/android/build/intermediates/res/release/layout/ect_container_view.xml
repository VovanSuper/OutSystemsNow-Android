<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:weightSum="1"
    tools:context="com.outsystems.android.mobileect.view.OSECTContainer"
    android:id="@+id/ectContainerFrag">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ectScreenContainer"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@android:color/transparent">

        <com.outsystems.android.mobileect.view.OSCanvasView
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:id="@+id/ectScreenCapture"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <include
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ectStatusInclude"
        android:layout_alignParentBottom="true"
        layout="@layout/ect_status_view" />

    <include
        layout="@layout/ect_toolbar_view"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ectToolbarInclude"
        android:layout_alignParentBottom="true" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:id="@+id/ectHelperGroup">

        <ImageView
            android:id="@+id/ectHelperView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp" />
    </LinearLayout>

    <include
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/ect_navigation_view"
        android:id="@+id/ectNavigationInclude"
        android:layout_alignParentTop="true"/>


